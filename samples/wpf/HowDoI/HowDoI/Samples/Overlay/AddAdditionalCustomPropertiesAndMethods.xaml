<UserControl x:Class="ThinkGeo.UI.Wpf.HowDoI.AddAdditionalCustomPropertiesAndMethods"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc1="clr-namespace:ThinkGeo.UI.Wpf;assembly=ThinkGeo.UI.Wpf">
    <Grid>
        <uc1:MapView x:Name="mapView" Loaded="MapView_Loaded"></uc1:MapView>
        <StackPanel Width="200" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="8" Opacity="0.95">
            <ComboBox Margin="0,5,0,0" x:Name="cboSecturityLevel" SelectionChanged="cboSecturityLevel_SelectionChanged">
                <ComboBoxItem ClipToBounds="False" IsSelected="True">AdministrationLevel</ComboBoxItem>
                <ComboBoxItem>AverageUsageLevel1</ComboBoxItem>
                <ComboBoxItem>AverageUsageLevel2</ComboBoxItem>
            </ComboBox>
        </StackPanel>
    </Grid>
</UserControl>
