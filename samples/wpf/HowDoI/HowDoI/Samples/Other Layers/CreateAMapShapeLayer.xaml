<UserControl x:Class="ThinkGeo.UI.Wpf.HowDoI.CreateAMapShapeLayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:uc1="clr-namespace:ThinkGeo.UI.Wpf;assembly=ThinkGeo.UI.Wpf">
    <Grid>
        <uc1:MapView Name="mapView" Loaded="mapView_Loaded"></uc1:MapView>
        <Button Height="28" Name="btnCalculate" VerticalAlignment="Top" HorizontalAlignment="Right" Width="246" Click="btnCalculate_Click" Content="Find Shortest Line from Point and Split Line" DataContext="{Binding}" Margin="0,113,15,0"/>
        <Button Height="28" HorizontalAlignment="Right" Margin="0,146,15,0" Name="btnClear" VerticalAlignment="Top" Width="246" Click="btnClear_Click" Content="Clear Results"></Button>
        <Button Content="Edit Test Features" Height="28" HorizontalAlignment="Right" Margin="0,180,15,0" Name="btnMovePoint" VerticalAlignment="Top" Width="246" Click="btnEdit_Click" />
    </Grid>
</UserControl>
