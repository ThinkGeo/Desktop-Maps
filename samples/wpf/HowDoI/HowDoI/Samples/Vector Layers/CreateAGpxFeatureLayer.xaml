<UserControl x:Class="ThinkGeo.UI.Wpf.HowDoI.CreateAGpxFeatureLayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:uc1="clr-namespace:ThinkGeo.UI.Wpf;assembly=ThinkGeo.UI.Wpf">
    <Grid>
        <uc1:MapView Name="mapView" Loaded="mapView_Loaded"></uc1:MapView>
        <StackPanel Width="200" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="8" Opacity="0.95">
            <TextBlock Margin="0,5,0,0" TextWrapping="Wrap" Height="20" VerticalAlignment="Bottom">Please select Gpx File:</TextBlock>
            <ComboBox HorizontalAlignment="Left" x:Name="sampleFileListBox" VerticalAlignment="Top" Width="195" SelectionChanged="sampleFileListBox_SelectionChanged" />
        </StackPanel>
    </Grid>
</UserControl>
