<UserControl x:Class="ThinkGeo.UI.Wpf.HowDoI.LoadAWmsImage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc1="clr-namespace:ThinkGeo.UI.Wpf;assembly=ThinkGeo.UI.Wpf">
    <Grid>
        <uc1:MapView x:Name="mapView" Loaded="MapView_Loaded"></uc1:MapView>
        <StackPanel Width="200" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="8" Opacity="0.95">
            <RadioButton GroupName="RequestMode" Margin="0 0 0 5" IsChecked="True" Checked="RadioButton_Checked">Use WmsOverlay</RadioButton>
            <RadioButton GroupName="RequestMode" Margin="0 0 0 5" Checked="RadioButton_Checked">Use WmsRasterLayer</RadioButton>
        </StackPanel>
    </Grid>
</UserControl>
