<Window x:Class="ClippingOnLineLayer.TestWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:my="clr-namespace:ThinkGeo.MapSuite.Wpf;assembly=ThinkGeo.MapSuite.Wpf"
    Title="Code Community-Clipping on Line Layer" Height="640" Width="800"
        Loaded="Window_Loaded" >
    <Grid>
        <my:WpfMap Name="wpfMap1" MouseMove="wpfMap1_MouseMove" />
        <TextBox Grid.Row="1" Name="textBox1" VerticalAlignment="Bottom" Grid.ColumnSpan="5" HorizontalAlignment="Left"/>
        <Button HorizontalAlignment="Right" Name="btnClip" VerticalAlignment="Bottom" Click="btnClip_Click">Clip Rail Roads based on Latvia</Button>
    </Grid>
</Window>
